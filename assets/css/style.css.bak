*{
	margin: 0px;
	padding: 0px;
	box-sizing: border-box;
}
a:link,
a:hover,
a:focus,
a:active{
	text-decoration: none;
}
.text-right{
	text-align: right;
}
.text-center{
	text-align: center;
}
.mb-63{
	margin-bottom: 63px;
}
.mb-78{
	margin-bottom: 78px;
}
.mb-13{
	margin-bottom: 13px;
}
.mb-34{
	margin-bottom: 34;
}
.mb-80{
	margin-bottom: 80px;
}
.mb-45{
	margin-bottom: 45px;
}
.mb-30{
	margin-bottom: 30px;
}
.mb-25{
	margin-bottom: 25px;
}
.mt-25{
	margin-top: 25px;
}
.mb-20{
	margin-bottom: 20px;
}
.btn{
	padding: 25px 70px;
	background-color: #00bc9c;
	font-size: 18px;
	color: #fff;
	font-weight: bold;
	border: 0px;
	text-transform: uppercase;
	cursor: pointer;
	border-radius: 5px;
	font-family: 'Source Sans Pro', sans-serif;
}
.btn:hover{
	color: #00bc9c;
	background-color: #fff;
}
/* ----------------header section-------------- */

.social-icon{
	background-color: #f5f7fa;
	padding: 15px 0px;
	/* width: 100%; */
}

.container{
	width: 1170px;
	margin: 0 auto;
}
.row,
.row::after,
.row::before{
	width: 100%;
	clear: both;
	display: table;
}
.col6, .col2, .col8, .col3, .col4, .col9{
	float: left;
	padding: 0px 15px;
}
.col3{
	width: calc((100% * 3) / 12);
}
.col4{
	width: calc((100% * 4) / 12);
}
.col9{
	width: calc((100% * 9) / 12);
}
.col2{
	width: calc((100% * 2) / 12);
}

.col8{
	width: calc((100% * 8) / 12);
}
.col6{
	width: 50%;
}
.mlgrid2{
	margin-left: calc((100% * 2) / 12);
}
.contact li{
	list-style: none;
	margin-right: 10px;
	display: inline-block;
	font-family: 'Source Sans Pro', sans-serif;
	
}
.contact li a{
	font: 12px;
	color: #506a85;
	padding-right: 4px;
}

.navigation{
	padding: 36px 0px;
	background-color: #fff;
}
.logo img{
	margin-right: 63px;
}
.navbar ul li{
	list-style: none;
	display: inline-block;
	margin: 6px;
	text-align: center;
}	
.search-icon li a, .navbar li a{
	color: #506a85;
	font-size: 14px;
}
.navbar	li a{
	font-weight: bold;
	padding: 6px;
	text-transform: uppercase;
	font-family: 'Source Sans Pro', sans-serif;
}
.search-icon li{
	list-style: none;
	margin-right: 22px;
}
/* ----------------------hreo image section css---------------------------*/
.backgroundimg img{
	width: 100%;
	height: 100vh;
	background: cover;
}
.heroimg{
	position: relative;
}
.caption{
	position: absolute;
	left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}
.sec-heading{
	font-size: 62px;
	color: #fff;
	text-transform: uppercase;
	font-family: 'Lato', sans-serif;
}
.sec-para{
	font-size: 16px;
	color: #a3b1bf;
	font-weight: lighter;
	font-family: 'Source Sans Pro', sans-serif;
	font-family: 'Lato', sans-serif;
}

.beautiful{
	padding: 130px 0px 130px;
	background-color: #fff;
}
.sec-heading2{
	color: #506a85;
	font-size: 44px;
	text-transform: capitalize;
	margin-bottom: 36px;
	font-family: 'Lato', sans-serif;
}
.sec-para2{
	font-size: 21px;
	color: #73879c;
	line-height: 1.5;
	font-family: 'Source Sans Pro', sans-serif;
	
}
.sec-heading3{
	color: #506a85;
	font-size: 26px;
	margin-bottom: 32px;
	font-family: 'Lato', sans-serif;
}
.sec-para3{
	font-size: 16px;
	color: #73879c;
	font-weight: lighter;
	font-family: 'Source Sans Pro', sans-serif;
}
.icon i{
	font-size: 60px;
	color: #00bc9c;
}
.chek-out{
	padding: 130px 0px 135px;
	background-color: #f2f5f7;
}

/* ------------------thumb ection----------------------------- */

.thumbimg  img{
	width: 555px; 
	height: 392px;
	/* margin-bottom: 34px; */
}
.img-info{
	background-color: #fff;
		padding: 34px 0px 30px;

}
.img-info1{
	font-size: 20px;
	font-weight: bold;
	color: #506a85;
	font-family: 'Lato', sans-serif;
}
.label{
	font-size: 14px;
	font-weight: lighter;
	color: #73879c;
	font-family: 'Source Sans Pro', sans-serif;
}
.dot{
	height: 15px;
	width: 15px;
	border-radius: 25px;
	background-color: inset 0 0 0 #000;
	display: inline-block;
	cursor: pointer;
}
.dot:hover{
	/* background-color: #00bc9c;	 */
	box-shadow: inset 88px 0 0 0 #000;
}

.work{
	padding: 130px 0px;
}
/* ----------------------------footer1 section scc------------------------------- */

.footer1{
	padding: 75px 0px 80px;
	background-color: #2f4052;
}
.f-heading{
	font-size: 16px;
	font-weight: bold;
	color: #fff;
	text-transform: uppercase;
	font-family: 'Lato', sans-serif;
}
.f-list li:last-child{
	margin-bottom: 25px
}
.f-list li, .about-us li{
	list-style: none;
}
.f-list li a{
	font-size: 14px;
	font-weight: lighter;
	color: #a3b1bf;
	font-family: 'Source Sans Pro', sans-serif;
}
.about-us li a{
	font-size: 14px;
	font-weight: bold;
	color: #a3b1bf;
	font-family: 'Lato', sans-serif;
}
.f-img li, .f2-info li, .f2-info2 li{
	list-style: none;
	display: inline-block;
}
.f-searchbar input{
	padding: 14px 12px;
	width: 100%;
	border: 0px;
	border-style: none;
	border-radius: 5px;
	position: relative;
}
.subscribe{
	padding: 10px 10px;
	background-color: #00bb9b;
	cursor: pointer;
	border-radius: 5px;
	border: 0px;
	position: absolute;
	    transform: translate(175px, -40px);
		color: #fff;
		font-size: 13px;
		font-weight: bold;
		text-transform: uppercase;
		font-family: 'Source Sans Pro', sans-serif;
}
.subscribe:hover{
	color: #00bc9c;
	background-color: #fff;
}
.footer2{
	padding: 35px 0px;
	background-color: #273545;
}
.f2-info li a, .f2-info2 li a{
	font-size: 14px;
	font-weight: lighter;
	color: #707e8c;
	font-family: 'Source Sans Pro', sans-serif;
}
.f2-info2 li a:last-child {
	padding-right: 0px;
}
.f2-info2 li a{
		padding: 0px 11px;
}
body{
	position: relative;
}
.corner-box{
	padding: 15px 12px 16px 12px;
	background-color: #3c5065;
	position: absolute;
	right: 20px;
	bottom: 20px;
	border-radius: 5px;
	color: #939aa2;
	position: fixed;
}